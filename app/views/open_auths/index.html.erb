<p id="notice"><%= notice %></p>

<h1>Listing Open Auths</h1>

<table>
  <thead>
    <tr>
      <th>Authorization uri</th>
      <th>Token credential uri</th>
      <th>Redirect uri</th>
      <th>Client</th>
      <th>Client secret</th>
      <th>Scope</th>
      <th>Code</th>
      <th>State</th>
      <th>Scope</th>
      <th>Access token</th>
      <th>Token type</th>
      <th>Expires in</th>
      <th>Update</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @open_auths.each do |open_auth| %>
      <tr>
        <td><%= open_auth.authorization_uri %></td>
        <td><%= open_auth.token_credential_uri %></td>
        <td><%= open_auth.redirect_uri %></td>
        <td><%= open_auth.client_id %></td>
        <td><%= open_auth.client_secret %></td>
        <td><%= open_auth.scope %></td>
        <td><%= open_auth.code %></td>
        <td><%= open_auth.state %></td>
        <td><%= open_auth.scope %></td>
        <td><%= open_auth.access_token %></td>
        <td><%= open_auth.token_type %></td>
        <td><%= open_auth.expires_in %></td>
        <td><%= open_auth.update %></td>
        <td><%= link_to 'Show', open_auth %></td>
        <td><%= link_to 'Edit', edit_open_auth_path(open_auth) %></td>
        <td><%= link_to 'Destroy', open_auth, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Open auth', new_open_auth_path %>
